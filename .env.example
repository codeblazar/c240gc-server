# .env

# Database
POSTGRES_USER=n8n
POSTGRES_PASSWORD=your_secure_password_here
POSTGRES_DB=n8ndb

# n8n config
N8N_BASIC_AUTH_ACTIVE=true
N8N_BASIC_AUTH_USER=your_username     # set your preferred username
N8N_BASIC_AUTH_PASSWORD=your_password   # choose a strong password

# SMTP disabled – lines commented out so n8n won’t send e‑mail
# N8N_SMTP_HOST=smtp.gmail.com
# N8N_SMTP_PORT=587
# N8N_SMTP_USER=your_email@gmail.com
# N8N_SMTP_PASS="***"
# N8N_SMTP_SENDER="n8n Admin <your_email@gmail.com>"
# N8N_SMTP_SSL=false
# N8N_SMTP_TLS=true

N8N_WEBHOOK_URL=https://your-domain.com
N8N_HOST=your-domain.com
N8N_PROTOCOL=https
N8N_PORT=5678
N8N_LOG_LEVEL=debug
N8N_USER_MANAGEMENT_DISABLED=false

FLOWISE_PORT=3000
FLOWISE_DATABASE_PATH=/data/flowise.db
FLOWISE_SECRETKEY_PATH=/data/.secret
FLOWISE_LOG_PATH=/data/logs
FLOWISE_BLOB_STORAGE_PATH=/data/storage